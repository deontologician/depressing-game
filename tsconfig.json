{
  "compilerOptions": {
    "module": "amd",
    "moduleResolution": "Node",
    "target": "es5",
    "sourceMap": true,
    "outFile": "depressing_game.js",

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true
  },
  "files": [
    "depressing_game.ts",
    "third-party/maquette.ts"
  ]
}
